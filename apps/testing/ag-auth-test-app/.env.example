# Database URL for Agentuity Auth
# Get your database URL from: agentuity cloud database list --region use --json
DATABASE_URL=postgresql://user:password@host:5432/database?sslmode=require

# Auth secret for signing tokens (min 32 characters)
# Generate with: openssl rand -hex 32
AGENTUITY_AUTH_SECRET=your-secret-key-at-least-32-characters
