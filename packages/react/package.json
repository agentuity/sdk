{
  "name": "@agentuity/react",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bun build --target browser --outdir dist src/index.ts --env AGENTUITY_PUBLIC_* && bunx tsc --build --force",
    "typecheck": "bunx tsc --noEmit",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "dependencies": {
    "@agentuity/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.0"
  }
}
