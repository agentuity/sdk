{
	"name": "@agentuity/react",
	"version": "0.0.12",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"AGENTS.md",
		"README.md",
		"dist"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"build": "bun build --target browser --outdir dist src/index.ts && bunx tsc --build --force",
		"typecheck": "bunx tsc --noEmit",
		"prepublishOnly": "bun run clean && bun run build"
	},
	"dependencies": {
		"@agentuity/core": "workspace:*"
	},
	"devDependencies": {
		"@types/react": "^19.0.0",
		"react": "^19.0.0",
		"typescript": "^5.9.0"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
