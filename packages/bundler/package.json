{
	"name": "@agentuity/bundler",
	"version": "0.0.5",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./dist/index.d.ts",
	"bin": {
		"agentuity-bundler": "./bin/bundler.ts"
	},
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"AGENTS.md",
		"README.md",
		"src",
		"dist",
		"bin"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"build": "bunx tsc --build --emitDeclarationOnly --force",
		"typecheck": "bunx tsc --noEmit",
		"prepublishOnly": "bun run clean && bun run build"
	},
	"dependencies": {
		"acorn-loose": "^8.5.2",
		"astring": "^1.9.0"
	},
	"devDependencies": {
		"typescript": "^5.9.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
