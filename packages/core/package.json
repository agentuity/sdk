{
	"name": "@agentuity/core",
	"version": "0.1.7",
	"license": "Apache-2.0",
	"author": "Agentuity employees and contributors",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"AGENTS.md",
		"README.md",
		"src",
		"dist"
	],
	"exports": {
		".": "./dist/index.js",
		"./workbench": "./dist/workbench.js"
	},
	"typesVersions": {
		"*": {
			".": [
				"./dist/index.d.ts"
			],
			"workbench": [
				"./dist/workbench.d.ts"
			]
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "bunx tsc --build --force",
		"typecheck": "bunx tsc --noEmit",
		"prepublishOnly": "bun run clean && bun run build"
	},
	"dependencies": {
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@agentuity/test-utils": "workspace:*",
		"@types/bun": "latest",
		"bun-types": "latest",
		"esbuild": "^0.25.0",
		"typescript": "^5.9.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}
