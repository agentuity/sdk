{
	"name": "create-agentuity",
	"version": "0.0.5",
	"module": "index.ts",
	"type": "module",
	"files": [
		"src",
		"app.ts",
		"AGENTS.md",
		"tsconfig.json",
		".gitignore",
		"README.md",
		"setup.ts"
	],
	"scripts": {
		"build": "bun ./node_modules/@agentuity/cli/bin/cli.ts bundle --dir . --dev",
		"dev": "bun run ./.agentuity/app.js",
		"typecheck": "bunx tsc --noEmit"
	},
	"keywords": [
		"agentuity",
		"create",
		"template",
		"scaffold",
		"bun"
	],
	"author": "Agentuity",
	"license": "MIT",
	"dependencies": {
		"@agentuity/cli": "workspace:*",
		"@agentuity/core": "workspace:*",
		"@agentuity/react": "workspace:*",
		"@agentuity/runtime": "workspace:*",
		"@hono/zod-validator": "^0.7.4",
		"enquirer": "^2.4.1",
		"hono": "^4.7.10",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"zod": "^4.1.12"
	},
	"bun-create": {
		"postinstall": [
			"bun run setup.ts",
			"bun run build"
		],
		"start": "bun run dev"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/react": "^19.0.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"publishConfig": {
		"access": "public"
	}
}
