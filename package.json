{
	"name": "@agentuity/sdk",
	"version": "0.0.5",
	"private": true,
	"workspaces": [
		"packages/*",
		"apps/*"
	],
	"scripts": {
		"build": "bunx tsc --build --force && bun run --workspaces build",
		"test": "bun test && cd apps/test-app && bun run test",
		"typecheck": "bunx tsc --build && cd apps/test-app && bunx tsc --build",
		"clean": "rm -rf packages/*/dist apps/*/dist packages/*/*.tsbuildinfo",
		"format": "bunx prettier */** --write --fix",
		"lint": "bunx eslint packages",
		"all": "bun clean && bun format && bun run build && bun lint && bun typecheck && bun run test"
	},
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@eslint/json": "^0.13.2",
		"@types/bun": "latest",
		"eslint": "^9.38.0",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.4.0",
		"jiti": "^2.6.1",
		"prettier": "3.6.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"typescript": "^5.9.0",
		"typescript-eslint": "^8.46.2"
	},
	"dependencies": {}
}
