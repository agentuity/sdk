{
	"name": "@agentuity/cli",
	"version": "0.0.107",
	"license": "Apache-2.0",
	"author": "Agentuity employees and contributors",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"agentuity": "./bin/cli.ts"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./vite-plugin": {
			"import": "./dist/cmd/build/vite/index.js",
			"types": "./dist/cmd/build/vite/index.d.ts"
		}
	},
	"files": [
		"AGENTS.md",
		"README.md",
		"src",
		"dist",
		"bin"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"build": "bunx tsc --build --force",
		"typecheck": "bunx tsc --noEmit",
		"test": "bun scripts/test-create-flow.ts && bun scripts/test-exit-codes.ts && bun scripts/test-response-schema.ts && bun scripts/test-batch-reporting.ts && bun scripts/test-response-envelope.ts",
		"test:create": "bun scripts/test-create-flow.ts",
		"test:exit-codes": "bun scripts/test-exit-codes.ts",
		"test:response-schema": "bun scripts/test-response-schema.ts",
		"test:batch": "bun scripts/test-batch-reporting.ts",
		"test:envelope": "bun scripts/test-response-envelope.ts",
		"test:bundled-create": "bun scripts/test-bundled-create.ts",
		"prepublishOnly": "bun run clean && bun run build"
	},
	"dependencies": {
		"@agentuity/core": "workspace:*",
		"@agentuity/server": "workspace:*",
		"@datasert/cronjs-parser": "^1.4.0",
		"@terascope/fetch-github-release": "^2.2.1",
		"@vitejs/plugin-react": "^5.1.2",
		"acorn-loose": "^8.5.2",
		"adm-zip": "^0.5.16",
		"astring": "^1.9.0",
		"cli-table3": "^0.6.5",
		"commander": "^14.0.2",
		"enquirer": "^2.4.1",
		"git-url-parse": "^16.1.0",
		"json-colorizer": "^3.0.1",
		"json5": "^2.2.3",
		"tar": "^7.5.2",
		"tar-fs": "^3.1.1",
		"typescript": "^5.9.0",
		"vite": "^7.2.7",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@agentuity/test-utils": "workspace:*",
		"@types/adm-zip": "^0.5.7",
		"@types/bun": "latest",
		"@types/tar-fs": "^2.0.4",
		"bun-plugin-tailwind": "^0.1.2",
		"bun-types": "latest",
		"hono": "^4.7.13",
		"tailwindcss": "^4.1.18"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}
